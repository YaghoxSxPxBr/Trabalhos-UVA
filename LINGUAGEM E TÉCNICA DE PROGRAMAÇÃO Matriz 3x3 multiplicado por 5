#include <stdio.h>
 #include <stdlib.h>
 int main(){
 int i, j, m[4][4];
 for(i=1;i<4;i++)
 for(j=1;j<4;j++){
 printf("Elemento[%d][%d]= ",i,j);
 scanf("%d",&m[i][j]);
 }
 printf("\nValores nÃ£o multiplicados    \n\n");
 for(i=1;i<4;i++){
 for(j=1;j<4;j++)
 printf("%d ",m[i][j]);
 printf("\n");
 }
 for(i=1;i<4;i++)
 for(j=1;j<4;j++)
 m[i][j]=m[i][j]*5;
 printf("\nValores Multiplicados   \n\n");
 for(i=1;i<4;i++){
 for(j=1;j<4;j++)
 printf("%d ",m[i][j]);
 printf("\n");
 }
 return 0;
 }
